{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "allowJs": false,
    "strict": true,
    "moduleResolution": "node",
    "skipLibCheck": true,
    "noUnusedLocals": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "sourceMap": false,
    "outDir": "dist",
    "types": [
      "vite/client"
    ],
    "baseUrl": ".",
    "paths": {
      "unplugin-dts/*": ["../unplugin-dts/src/*"],
      "unplugin-dts": ["../unplugin-dts/src"]
    }
  },
  "include": [
    "src"
  ],
  "exclude": [
    "node_modules"
  ]
}
